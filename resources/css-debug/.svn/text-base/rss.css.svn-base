#admob_ad{
	height:48px;
}

.feeds-list{
	padding: 0px;
	font-weight: bold;
	/*margin:10px 20px 20px;*/
	overflow: hidden;
	position: relative;
	background: #EEE;
}

.feeds-list .feed-logo{
	clear:both;
	float:left;
}

.feed-logo > img{
	background-color: white;
	height: 33px;
	padding: 2px;
	float:left;
}

.feeds-list .feed-bubble {
	-webkit-box-flex: 1;
	border-bottom-left-radius: 6px 6px;
	border-bottom-right-radius: 6px 6px;
	border-top-left-radius: 6px 6px;
	border-top-right-radius: 6px 6px;
	display: block;
	overflow: hidden;
	position: relative;
	clear:both;
	float:left;
}

.feeds-list .feed-content {
	color: #444;
	padding: 5px;
	text-shadow: white 0px 1px 0px;
	z-index: 1;
}

.feeds-list .feed-content h2 {
	color: #111;
	font-weight: bold;
	font-size: 0.7em;
}

.feeds-list .feed-content p {
	font-size: 0.6em;
}



.feeds-list .delete{
	background-image: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#D17E84), to(#BD1421));
	border-color: #555;
	float: right;
	font-size: 0.7em;
	padding: 5px 8px;
	color: white;
	margin-right: 10px;
}

.delete-item-active .feeds-list .action{
	-webkit-transition: opacity 0.3s linear;
	opacity: 1;
	font-weight: normal;
} 

.feeds-list .action{
	opacity: 0;
}

