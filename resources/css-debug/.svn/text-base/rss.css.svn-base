#feed-title .x-toolbar-title{
	text-overflow: ellipsis;
	/*overflow: hidden;
	white-space:nowrap;*/
	display:block;
	width:150px;
	margin-left: auto;
	margin-right: auto;
	font-size: 0.7em;
	word-wrap: break-word;
	word-break:normal;
}

.feed span{
	font-size: 0.7em;
}




.group-feed-widget{
	display: inline-block;
	height: 80px;
	width: 80px;
}

.outerWrapper{
	margin: 10px;
}

.widget{
	margin: 10px;
	-webkit-box-sizing: border-box;
	-webkit-transition: -webkit-transform 0.3s ease-in-out;
	/*background-image: -webkit-gradient(radial, 50% 60%, 0, 50% 50%, 20, from(rgba(0, 5, 8, 0.496094)), to(rgba(0, 5, 8, 0)));*/
	background-position: 0% 80%;
	background-repeat: no-repeat;
	background-size: 100% ;
	border-bottom-left-radius: 10px 10px;
	border-bottom-right-radius: 10px 10px;
	border-top-left-radius: 10px 10px;
	border-top-right-radius: 10px 10px;
	display: block;
	margin: 18px 19px 5px 18px;
}

.widget-title{
	color: black;
	font-family: 'Lucida Grande', Verdana, Helvetica, Arial, sans-serif;
	font-size: 11px;
	font-weight: bolder;
	letter-spacing: -0.05em;
	margin: 0px;
	text-align: center;
}

img.x-icon-mask{
	-webkit-mask-size: 2.4em 2.4em;
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FCFCFC), color-stop(0.45, #CCC), to(#F6F6F6));
	/*background-image: url(http://upload.wikimedia.org/wikipedia/commons/thumb/4/43/Feed-icon.svg/128px-Feed-icon.svg.png) center center no-repeat;*/
  	background-size: 100%;
	height: 2.4em;
	/*margin: 0.4em auto;*/
	width: 2.4em;
}


#carousel .x-item-selected img.x-icon-mask{
	-webkit-mask-size: 2.4em 2.4em;
	background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#FFFFCC), color-stop(0.45, #CCC), to(#FF9900));
	/*background-image: url(http://upload.wikimedia.org/wikipedia/commons/thumb/4/43/Feed-icon.svg/128px-Feed-icon.svg.png) center center no-repeat;*/
  	background-size: 100%;
	height: 2.4em;
	/*margin: 0.4em auto;*/
	width: 2.4em;
}
